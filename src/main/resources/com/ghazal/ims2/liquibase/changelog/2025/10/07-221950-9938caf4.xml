    <changeSet id="1" author="IMS2">
        <createTable tableName="ITEM_UNIT_PRICE">
            <column name="ID" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_ITEM_UNIT_PRICE"/>
