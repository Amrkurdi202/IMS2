    </changeSet>
    <changeSet id="15" author="IMS2">
        <addUniqueConstraint columnNames="rooms_ID" constraintName="UC_PROJECT_ROOMS_ROOMS" tableName="PROJECT_rooms"/>
    </changeSet>
    <changeSet id="16" author="IMS2">
        <addUniqueConstraint columnNames="points_id" constraintName="UC_ROOM_POINTS_POINTS" tableName="ROOM_points"/>
    </changeSet>
